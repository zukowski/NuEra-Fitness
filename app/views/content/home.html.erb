<script type="text/javascript">
$(document).ready(function() {
	$("a.home-page-menu").hover(
		function() {
			$(this).find("span.title").hide();
			$(this).find("span.title-hover").show();
		},
		function() {
			$(this).find("span.title-hover").hide();
		  $(this).find("span.title").show();
		});
		
		bg = new Image();
		bg.src = "/images/menu-button-bg-left-hover.png";
		$("h2.raised").css('z-index','3').hide();
		$("div.category").hover(
			function() {
				$(this).find("h2.raised").show();
				$(this).find("h2.lowered").hide();
				$(this).find("h2.raised *").css('visibility','visible');
				$(this).find("h2.raised").css('z-index','5');
			},
			function() {
				$(this).find("h2.raised").hide();
				$(this).find("h2.lowered").show();
				$(this).find("h2.raised *").css('visibility','hidden');
				$(this).find("h2.raised").css('z-index','3');
			}
		);
		
		$(".face").hover(
			function() {
				$(this).find("img").show();
			},
			function() {
				$(this).find("img").hide();
			}
		);
		
		$('.slideshow').cycle({
			fx: 'fade',
			speed: 400,
			timeout: 5000
			// choose your transition type, ex: fade, scrollUp, shuffle, etc...
		});
		
		$(".twitter").tweet({
			join_text: "auto",
			username: "NuEraFitness",
			avatar_size: 36,
			count: 2,
			auto_join_text_default: "we said,",
			auto_join_text_ed: "we",
			auto_join_text_ing: "we were",
			auto_join_text_reply: "we replied",
			auto_join_text_url: "we were checking out",
			loading_text: "loading tweets..."
		});
});
</script>

